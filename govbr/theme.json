		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"customSpacingSize": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "Base",
					"size": "8px",
					"slug": "scale-base"
				},
				{
					"name": "2x Greater",
					"size": "16px",
					"slug": "scale-up-2"
				},
				{
					"name": "3x Greater",
					"size": "24px",
					"slug": "scale-up-3"
				},
				{
					"name": "4x Greater",
					"size": "36px",
					"slug": "scale-up-4"
				},
				{
