	"styles": {
		"color": {
			"background": "var(--wp--preset--color--green)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)",
			"fontSize": "var(--wp--preset--font-size--scale-base, 1em)",
			"lineHeight": "var(--wp--custom--line-height--medium, 1.45)"
		},
		"spacing": {
			"blockGap": "clamp(var(--wp--preset--spacing--scale-up-2, 16px), 5vw, var(--wp--preset--spacing--scale-up-2-h, 20px))",
			"padding": {
				"top": "var(--wp--preset--spacing--scale-up-2, 16px)",
				"right": "clamp(var(--wp--preset--spacing--scale-up-2, 16px), 5vw, var(--wp--preset--spacing--scale-up-5, 40px))",
				"bottom": "var(--wp--preset--spacing--scale-up-2, 16px)",
				"left": "clamp(var(--wp--preset--spacing--scale-up-2, 16px), 5vw, var(--wp--preset--spacing--scale-up-5, 40px))"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-06, 2.986em)",
					"lineHeight": "var(--wp--custom--line-height--page-title, 1.1)",
					"fontWeight": "var(--wp--custom--font-weight--light)"
				},
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "var(--wp--custom--spacing--scale-up-2-h, 20px)"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-05, 2.488em)",
					"lineHeight": "var(--wp--custom--line-height--low, 1.15)",
					"fontWeight": "var(--wp--custom--font-weight--regular, 400)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-04, 2.074em)",
					"lineHeight": "var(--wp--custom--line-height--low, 1.15)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-03, 1.728em)",
					"lineHeight": "var(--wp--custom--line-height--low, 1.15)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-02, 1.44em)",
					"lineHeight": "var(--wp--custom--line-height--low, 1.15)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--wp--custom--line-height--low, 1.15)",
					"fontWeight": "var(--wp--custom--font-weight--extra-bold)",
					"textTransform": "uppercase"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--wp--custom--line-height--low, 1.15)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			}
		},
		"blocks": {
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-02, 1.44em)",
					"lineHeight": "1.4"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--gray)",
					"text": "var(--wp--preset--color--green)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-base, 1em)",
					"fontWeight": "500"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-02, 1.44em)"
				}
			},
			"core/comment-date": {
