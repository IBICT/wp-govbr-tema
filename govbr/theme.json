		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.125rem, 5vw, 1.375rem)",
					"slug": "20",
					"name": "1"
				},
				{
					"size": "clamp(1.4375em, 6vw, 1.625rem)",
					"slug": "25",
					"name": "2"
				},
				{
					"size": "clamp(1.75rem, 6vw, 2rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "7"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"custom": {
			"font-primary": "\"Rawline\", \"Raleway\", sans-serif",
			"line-height": {
				"body": 1.7,
				"heading": 1.3,
				"page-title": 1.1
			},
			"font-weight":{
				"light": "300",
				"regular": "400",
				"medium": "500",
				"semi-bold": "600",
				"bold": "700",
				"extra-bold": "800",
				"black": "900"
			}
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--green)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "var(--wp--custom--line-height--body, 1.7)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"top": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge, 64px)",
					"lineHeight": "var(--wp--custom--line-height--page-title, 1.1)",
					"fontWeight": "var(--wp--custom--font-weight--light)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large, 36px)",
					"lineHeight": "var(--wp--custom--line-height--heading, 1.3)",
					"fontWeight": "var(--wp--custom--font-weight--regular, 400)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "calc(1.25 * var(--wp--preset--font-size--large, 24px))",
					"lineHeight": "var(--wp--custom--line-height--heading, 1.3)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large, 24px)",
					"lineHeight": "var(--wp--custom--line-height--heading, 1.3)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)",
					"lineHeight": "var(--wp--custom--line-height--heading, 1.3)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small, 16px)",
					"lineHeight": "var(--wp--custom--line-height--heading, 1.3)",
					"fontWeight": "var(--wp--custom--font-weight--extra-bold)"
				}
			}
		},
		"blocks": {
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)",
					"lineHeight": "1.4"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--gray)",
					"text": "var(--wp--preset--color--green)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "500"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small, 16px)",
					"lineHeight": "var(--wp--custom--line-height--body, 1.7)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small, 16px)",
					"lineHeight": "var(--wp--custom--line-height--body, 1.7)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small, 16px)",
					"lineHeight": "var(--wp--custom--line-height--body, 1.7)"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small, 18px)",
					"fontWeight": "700"
				},
				"border": {
					"radius": "0px",
					"style": "solid none",
					"width": "var(--wp--custom--border--width)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large, 24px)",
					"fontWeight": "var(--wp--custom--font-weight--regular, 400)",
					"textTransform": "uppercase"
				}
			},
			"core/code": {
				"border": {
					"radius": "0px",
					"color": "var(--wp--preset--color--dark-gray)",
					"style": "solid",
					"width": "1.5px"
				}
			}
		}
	}
}