			"minHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--background)",
			"text": "var(-color)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)",
			"fontSize": "var(--wp--preset--font-size--scale-base, 1em)",
			"lineHeight": "var(--font-line-height-medium, 1.45)"
		},
		"spacing": {
			"blockGap": "clamp(var(--wp--preset--spacing--scale-up-2, 16px), 5vw, var(--wp--preset--spacing--scale-up-2-h, 20px))",
			"padding": {
				"top": "var(--wp--preset--spacing--scale-up-2, 16px)",
				"right": "clamp(var(--wp--preset--spacing--scale-up-2, 16px), 5vw, var(--wp--preset--spacing--scale-up-5, 40px))",
				"bottom": "var(--wp--preset--spacing--scale-up-2, 16px)",
				"left": "clamp(var(--wp--preset--spacing--scale-up-2, 16px), 5vw, var(--wp--preset--spacing--scale-up-5, 40px))"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--interactive)"
				},
				":hover": {
					"color": {
						"gradient": "linear-gradient(rgba(var(--interactive-rgb),var(--hover)),rgba(var(--interactive-rgb),var(--hover)))"
					}
				},
				":active": {
					"color": {
						"gradient": "linear-gradient(rgba(var(--interactive-rgb),var(--pressed)),rgba(var(--interactive-rgb),var(--pressed)))"
					}
				},
				":focus": {
					"outline": {
						"color": "var(--focus)",
						"offset": "var(--focus-offset)",
						"style": "var(--focus-style)",
						"width": "var(--focus-width)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-06, 2.986em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": "var(--font-weight-light)"
				},
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "var(--spacing--scale-up-2-h, 20px)"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-05, 2.488em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": "var(--font-weight-regular, 400)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-04, 2.074em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": "var(--font-weight-medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-03, 1.728em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": ""
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-02, 1.44em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": "var(--font-weight-bold)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": "var(--font-weight-extra-bold)",
					"textTransform": "uppercase"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--font-line-height-low, 1.15)",
					"fontWeight": "var(--font-weight-semi-bold)"
				}
			}
		},
		"blocks": {
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-02, 1.44em)",
					"lineHeight": "1.4"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--button-radius, 100em)",
					"style": "solid",
					"color": "var(--interactive)",
					"width": "0px"
				},
				"color": {
					"background": "var(--interactive-light)",
					"text": "var(--color-dark)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"fontWeight": "var(--font-weight-semi-bold)"
				},
				"spacing": {
					"padding": {
						"top": "0",
						"bottom": "0",
						"left": "var(--spacing-scale-3x)",
						"right": "var(--spacing-scale-3x)"
					}
				},
				"variations": {
					"outline": {
						"border": {
							"width": "1px",
							"style": "solid",
							"color": "var(--interactive)"
						},
						"color": {
							"background": "var(--background-light)",
							"text": "var(--interactive)"
						}
					}
				}
			},
