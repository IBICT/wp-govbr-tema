			"font-primary": "\"Rawline\", \"Raleway\", sans-serif",
			"line-height": {
				"low": 1.15,
				"medium": 1.45,
				"high": 1.85
			},
			"font-weight":{
				"light": "300",
				"regular": "400",
				"medium": "500",
				"semi-bold": "600",
				"bold": "700",
				"extra-bold": "800",
				"black": "900"
			},
			"spacing": {
				"scale-up-8": "64px",
				"scale-up-9": "72px",
				"scale-up-10": "80px",
				"scale-up-half": "4px",
				"scale-up-baseh": "12px",
				"scale-up-2-h": "20px",
				"scale-up-3-h": "28px",
				"scale-up-4-h": "36px",
				"scale-up-5-h": "44px",
				"scale-up-6-h": "52px",
				"scale-up-7-h": "60px",
				"scale-up-8-h": "68px",
				"scale-up-9-h": "76px",
				"scale-up-10-h": "84px"
			}
		},
		"useRootPaddingAwareAlignments": true
