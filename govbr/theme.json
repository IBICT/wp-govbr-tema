			],
			"gradients": [
				{
					"slug": "white-to-light",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--surface-light), var(--wp--preset--color--surface-light-alternative))",
					"name": "White to Light"
				},
				{
					"slug": "white-and-light",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--surface-light) 50%, var(--wp--preset--color--surface-light-alternative) 50%)",
					"name": "White and Light"
				},
				{
					"slug": "light-to-white",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--surface-light-alternative), var(--wp--preset--color--surface-light))",
					"name": "Light to White"
				},
