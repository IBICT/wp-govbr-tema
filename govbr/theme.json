					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--wp--custom--line-height--medium, 1.45)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--wp--custom--line-height--medium, 1.45)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-01, 1.2em)",
					"lineHeight": "var(--wp--custom--line-height--medium, 1.45)"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-02, 1.44em)",
					"fontWeight": "700"
				},
				"border": {
					"radius": "0px",
					"style": "solid none",
					"width": "var(--wp--custom--border--width)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--scale-up-03, 1.728em)",
					"fontWeight": "var(--wp--custom--font-weight--regular, 400)",
					"textTransform": "uppercase"
				}
			},
			"core/code": {
				"border": {
					"radius": "0px",
					"color": "var(--wp--preset--color--dark-gray)",
					"style": "solid",
					"width": "1.5px"
				}
			}
		}
	}
}