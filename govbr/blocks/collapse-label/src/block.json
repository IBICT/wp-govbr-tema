{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "govbr/collapse-label",
	"title": "Collapse label",
	"category": "widgets",
	"description": "Label for the collapse component.",
	"usesContext": [ "govbr/blockId" ],
	"parent": [ "govbr/collapse" ],
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "div.flex-fill",
			"default": "",
			"__experimentalRole": "content"
		},
		"textAlign": {
			"type": "string"
		},
		"level": {
			"type": "number",
			"default": 2
		},
		"parentBlockId": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"anchor": true,
		"html": false,
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"fontAppearance": true,
				"textTransform": true
			}
		},
		"__unstablePasteTextInline": true,
		"__experimentalSlashInserter": true
	},
	"textdomain": "govbr",
	"editorScript": "file:./index.js",
	"style": "file:./index.css"
}
